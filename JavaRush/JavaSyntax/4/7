package com.javarush.task.pro.task04.task0413;

/* 
Рисуем треугольник
*/

public class Solution {
    public static void main(String[] args) {
        //напишите тут ваш код
        for(int n = 0;n<10;n++)
        {
            for(int m = 0; m<=(0+n);m++)
            {
                System.out.print(8);
            }
            System.out.println();
        }
    }
}
